<div class="row row-cols-1 row-cols-md-3 g-4">
  <% CardFish.all.each do |f| %>
    <div class="col">
      <div class="card">
          <img src="..." class="card-img-top" alt="...">
          <div class="card-body">
            <h5 class="card-title"><%= f.name %></h5>
            <p class="card-text"><%= f.description %></p>
          </div>
      </div>
    </div>
  <% end %>
</div>
